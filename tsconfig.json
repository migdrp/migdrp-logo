{
	"compilerOptions": {
    "experimentalDecorators": true,
	  "module": "commonjs",
	  "esModuleInterop": true,
	  "allowSyntheticDefaultImports": true,
	  "noImplicitAny":false,
		"removeComments":true,
		"sourceMap": true,
    "target": "es5",
	  "moduleResolution": "node",
	  "outDir": "./dist",
		"resolveJsonModule": true,
	},
	"include": ["./src/**/*", "test"],
	"exclude": ["node_modules"]
}




