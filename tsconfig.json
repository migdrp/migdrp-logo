{
	"compilerOptions": {
    "experimentalDecorators": true,
	  "module": "commonjs",
	  "esModuleInterop": true,
	  "allowSyntheticDefaultImports": true,
	  "noImplicitAny":false,
		"removeComments":true,
		"sourceMap": true,
    "target": "es6",
	  "moduleResolution": "node",
	  "outDir": "./dist",
		"resolveJsonModule": true
	},
	"include": ["./src/**/*"],
	"exclude": ["node_modules"]
}




